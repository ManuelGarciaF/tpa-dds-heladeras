<!-- Partial para formularios de colaboraciones -->
{{#>templates/partials/layout}}
    {{>templates/partials/nav}}
    <div class="vw-100 py-4"
         style="background-image: url({{image-url}});
                 background-size: cover;
                 background-color: #0a4861;
                 ">
        <h3 class="display-4 text-center py-4 text-uppercase fw-medium text-light w-75 mx-auto">
            {{title}}
        </h3>
    </div>
    <div class="container-lg content-container border border-3 rounded-4 bg-body p-3 mt-5">
        {{#each errors}}
            <div class="alert alert-danger" role="alert">
                {{this}}
            </div>
        {{/each}}
        {{>@partial-block}}
    </div>
{{/templates/partials/layout}}